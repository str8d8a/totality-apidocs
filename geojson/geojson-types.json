{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "http://geojson.org/schema/geojson-types",
  "title": "GeoJSON types",
  "description": "The value values for the \"type\" property of GeoJSON Objects.",
  "link": "https://tools.ietf.org/html/rfc7946#section-1.4",
  "type": {
    "enum": [
      "Point",
      "LineString",
      "MultiPoint",
      "Polygon",
      "MultiLineString",
      "MultiPolygon",
      "GeometryCollection",
      "FeatureCollection",
      "Feature"
    ]
  }
}